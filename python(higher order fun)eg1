def values(fnc,x,y):
 print(fnc(x,y))
def mul(x,y):
  for i in range(1,11):
    print(i,'*',x,'=',x*i)
  for i in range(1,11):
    print(i,'*',y,'=',i*y)
values(mul,2,3)
1 * 2 = 2
2 * 2 = 4
3 * 2 = 6
4 * 2 = 8
5 * 2 = 10
6 * 2 = 12
7 * 2 = 14
8 * 2 = 16
9 * 2 = 18
10 * 2 = 20
