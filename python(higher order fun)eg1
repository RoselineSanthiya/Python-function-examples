10 * 3 = 30

def values(nos,x,y):
  print(nos(x,y))
def nos(x,y):
  for i in range(1,11):
   print(i,'*',x,'=',i*x)
  for i in range(1,11):
   print(i,'*',y,'=',i*y)
values(nos,2,4)
1 * 2 = 2
2 * 2 = 4
