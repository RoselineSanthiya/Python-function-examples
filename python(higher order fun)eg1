def division(nos,x):
  print(nos(x))
def nos(x):
  for i in range(1,11):
    print(i,'/',x,'=',i/x)
division(nos,3)
division(nos,5)
1 / 3 = 0.3333333333333333
2 / 3 = 0.6666666666666666
3 / 3 = 1.0
4 / 3 = 1.3333333333333333
5 / 3 = 1.6666666666666667
6 / 3 = 2.0
7 / 3 = 2.3333333333333335
8 / 3 = 2.6666666666666665
9 / 3 = 3.0
10 / 3 = 3.3333333333333335
None
