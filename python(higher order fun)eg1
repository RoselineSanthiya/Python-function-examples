def a():
   print('bru coffee')
def b():
   print('sunrise coffee')
def callab(coffee):
   coffee()
callab(a)
callab(b)
bru coffee
sunrise coffee

def a():
  print('a fun')
def b():
  print('b fun')
def callab(fun):
  fun()
callab(a)
a fun

